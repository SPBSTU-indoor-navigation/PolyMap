--exclude Pods,**/Generated
--disable redundantRawValues,blankLinesAtStartOfScope,consecutiveBlankLines,blankLinesAtEndOfScope,wrapMultilineStatementBraces,consecutiveSpaces,redundantReturn


--maxwidth none
--allman false
--xcodeindentation disabled

--trimwhitespace nonblank-lines
--commas inline
--lineaftermarks false
--ranges nospace
--wrapparameters after-first

--modifierorder static,public,public(set)private,private(set),open,fileprivate,internal
--nospaceoperators ...,..<

--nevertrailing map,filter,reduce,compactMap,compactMapValues,flatMap
